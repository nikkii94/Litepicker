{
  "compilerOptions": {
    "outDir": "./dist/js",
     "module": "amd",
    "downlevelIteration": true,
    "lib": [
      "es5", "es6", "dom", "dom.iterable"
    ],
    "target": "es5",
    "declaration": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "src/methods.ts",
    "tests"
  ]
}
